{
  "expo": {
    "name": "promiseUnlocked",
    "slug": "promiseunlocked",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to take photos of your academic transcript.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to select images of your academic transcript.",
        "NSMicrophoneUsageDescription": "We use the mic to transcribe your speech on device.",
        "NSSpeechRecognitionUsageDescription": "We use speech recognition to transcribe audio on device."
      },
      "bundleIdentifier": "com.anonymous.TranscriptAnalyzer"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.TranscriptAnalyzer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you share photos with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6ecf846e-45ab-42ed-b633-febe4811c82f"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/6ecf846e-45ab-42ed-b633-febe4811c82f"
    }
  }
}
